#include<stdio.h>
int fact(int);  
int main() 
{ 
int n,f; //n is the number f to store the sum 
printf("enter the no"); 
scanf("%d",&n); 
f=fact(n); 
printf("factorial %d",f); 
} 
int fact(int n) //fact function 
{ 
if(n==0) // checking if the number is 0 and return 0 
return 0; 
else if(n==1) // checking if the number is 1 and return 1  
return 1; 
else 
return n*fact(n-1); //returning the value above 1  
}
